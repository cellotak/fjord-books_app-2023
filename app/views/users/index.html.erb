<h1>Users#index</h1>
<p>Find me in app/views/users/index.html.erb</p>

<div id="users" class="index-items">
  <% @users.each do |user| %>
    <div class="index-item">
      <p>
        <%= user.email %>
      </p>    
    </div>
  <% end %>
</div>


<% @users.each do |user| %>
  <table class="table">
    <tr>
    <th>名前</th>
    <th>作成日時</th>
  </tr>
  <tr>
    <td><%= user.email %></td>
    <td><%= user.created_at %></td>
  </tr>
  </table>
<% end %>